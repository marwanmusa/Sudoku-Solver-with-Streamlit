    "def import_boardsudoku(filename):\n",
    "    file = open(filename,'r')\n",
    "    content = file.read()\n",
    "    file.close()\n",
    "    return list(map(int, content.replace(\"\\n\", \"\").split(',')))\n",
    "\n",
    "print(import_boardsudoku(\"sudoku_hs.txt\"))"
   ]
  }
